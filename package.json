{
  "name": "@kiket-dev/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/Node.js SDK for building Kiket extensions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "kiket",
    "extensions",
    "webhooks",
    "workflow",
    "automation"
  ],
  "author": "Kiket Team <team@kiket.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiket-dev/kiket.git",
    "directory": "sdk/nodejs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1"
  }
}
